<Application x:Class="MonitorWindows.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml"
             ShutdownMode="OnLastWindowClose">
    
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="./Resources/StylesDictionary.xaml" />
                <ResourceDictionary Source="Resources/StringResource.xaml" />
                <ResourceDictionary Source="Resources/ExpanderDictionary.xaml" />
                <ResourceDictionary Source="Resources/ComboBoxDictionary.xaml" />
                <ResourceDictionary Source="Resources/DataGridDictionary.xaml" />
                <ResourceDictionary Source="Resources/ButtonDictionary.xaml" />
                <ResourceDictionary Source="Resources/TabControlDictionary.xaml" />
                <ResourceDictionary Source="Resources/RadioButtonDictionary.xaml" />
                <ResourceDictionary Source="Resources/CheckBoxDictionary.xaml" />
                <ResourceDictionary Source="Resources/ImageDictionary.xaml" />
                <ResourceDictionary Source="Resources/GroupBoxDictionary.xaml" />
                <ResourceDictionary Source="Resources/SliderDictionary.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
